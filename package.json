  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:testing-library/react"
    ],
    "plugins": [
      "@typescript-eslint",
      "react",
      "import",
      "react-hooks",
      "jsx-a11y",
      "testing-library"
    ],
    "settings": {
      "import/resolver": {
